<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="ansible.config.modules_dir"/>
        <parameter key="ansible.config.default_author">%env(AUTHOR)%</parameter>
    </parameters>
    <services>
        <service id="ansible.command.create_modules" class="RouterOS\Generator\Provider\Ansible\Command\CreateModulesCommand">
            <tag name="console.command" />
        </service>
    </services>
</container>