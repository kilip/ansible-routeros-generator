{
    "name": "routeros/generator",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "league/html-to-markdown": "^4.10",
        "symfony/console": "5.1.*",
        "symfony/css-selector": "5.1.*",
        "symfony/dom-crawler": "5.1.*",
        "symfony/dotenv": "5.1.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.1.*",
        "symfony/http-client": "5.1.*",
        "symfony/twig-bundle": "5.1.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "5.1.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpstan/phpstan": "^0.12.42",
        "phpstan/phpstan-phpunit": "^0.12.16",
        "phpstan/phpstan-symfony": "^0.12.7",
        "symfony/phpunit-bridge": "dev-master"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "RouterOS\\Generator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\RouterOS\\Generator\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "scrap": "./bin/console routeros:scrap --ansi",
        "phpstan": "./vendor/bin/phpstan analyse --ansi",
        "phpcs": "./vendor/bin/php-cs-fixer fix --ansi",
        "phpunit": "phpunit --colors=always",
        "prepare-test": [
            "./bin/console doctrine:database:create --ansi --env=test",
            "./bin/console doctrine:schema:update --force --env=test"
        ],
        "test": [
            "@prepare-test",
            "@phpunit"
        ],
        "coverage": [
            "@prepare-test",
            "@phpunit --coverage-text --coverage-clover=var/log/clover.xml"
        ],
        "verify": [
            "@phpunit",
            "@phpcs --dry-run --diff",
            "@phpstan"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    }
}
